{
	"name": "my-fullstack-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"dev:turbo": "next dev --turbo",
		"dev:memory": "cross-env NODE_ENV=development NODE_OPTIONS=--max-old-space-size=4096 NEXT_TELEMETRY_DISABLED=1 next dev",
		"restart": "taskkill /f /im node.exe 2>NUL || echo No processes to kill",
		"build": "next build",
		"start": "next start",
		"build:prod": ".\\scripts\\test-production.bat",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
		"format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
		"preview": "next build && next start",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:seed": "tsx scripts/seed.ts",
		"db:create": "tsx scripts/create-database.ts",
		"type-check": "tsc --noEmit",
		"check": "pnpm type-check && pnpm lint && pnpm format:check",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"report": "node ./dist/scripts/generate-report.js",
		"report:dev": "tsx scripts/generate-report.ts"
	},
	"dependencies": {
		"@ant-design/icons": "^6.0.0",
		"@ant-design/nextjs-registry": "^1.1.0",
		"@ant-design/v5-patch-for-react-19": "^1.0.3",
		"antd": "^5.27.0",
		"bcryptjs": "^3.0.2",
		"clsx": "^2.0.0",
		"dayjs": "^1.11.13",
		"drizzle-kit": "^0.31.4",
		"drizzle-orm": "^0.44.4",
		"jose": "^6.0.12",
		"js-cookie": "^3.0.5",
		"next": "^15.1.0",
		"next-auth": "^4.24.11",
		"postgres": "^3.4.7",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@iconify-json/heroicons": "^1.1.21",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.0.0",
		"@types/js-cookie": "^3.0.6",
		"@types/next-auth": "^3.15.0",
		"@types/node": "^20.0.0",
		"@types/pg": "^8.15.5",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@unocss/cli": "^66.4.2",
		"@unocss/postcss": "^66.4.2",
		"@unocss/preset-icons": "^66.4.2",
		"@unocss/preset-uno": "^66.4.2",
		"@vitest/coverage-v8": "^2.0.5",
		"autoprefixer": "^10.4.16",
		"concurrently": "^8.2.2",
		"cross-env": "^10.0.0",
		"dotenv": "^17.2.1",
		"eslint": "^8.0.0",
		"eslint-config-next": "^15.1.0",
		"jsdom": "^24.1.0",
		"postcss": "^8.4.32",
		"prettier": "^3.0.0",
		"rimraf": "^6.0.1",
		"tsx": "^4.20.3",
		"typescript": "^5.0.0",
		"unocss": "^66.4.2",
		"vitest": "^2.0.5"
	}
}
